{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "sdb": "surreal start ",
    "start:dev": "dotenv -- turbo sdb dev",
    "sdb:schema": "surreal import ./src/schema/surreal/app.surql -e http://localhost:3303",
    "sdb:demo": "dotenv -- surreal import ./src/schema/surreal/demo.surql -e http://localhost:3303",
    "sdb:update": "dotenv -- turbo sdb:schema"
  },
  "dependencies": {
    "@nuxt/ui": "^2.17.0",
    "@pinia/nuxt": "^0.5.1",
    "argon2": "^0.40.3",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "nuxt": "^3.12.4",
    "pinia": "^2.1.7",
    "qs": "^6.12.3",
    "surrealdb.js": "0.11.0",
    "vue": "latest",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@iconify-json/ri": "^1.1.21",
    "@iconify-json/solar": "^1.1.9",
    "@types/argon2": "^0.15.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/qs": "^6.9.15",
    "dotenv-cli": "^7.3.0",
    "tailwindcss": "^3.4.6",
    "turbo": "1.12.2"
  }
}
